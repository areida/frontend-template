{
  "author": "Synapse Studios",
  "name": "frontend-template",
  "version": "0.0.0",
  "description": "A quickstart frontend template that expects to be paired with a backend based off of synapsestudios/api-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/synapsestudios/frontend-template.git"
  },
  "scripts": {
    "dev": "npm install && bower install && gulp watch",
    "production": "npm install && bower install && gulp clean && gulp build --build production",
    "sitemap": "npm install && bower install && node sitemap-gen.js > application/sitemap.xml"
  },
  "devDependencies": {
    "connect-modrewrite": "^0.6.4-pre",
    "browserify": "^3.43.0",
    "reactify": "^0.11.0",
    "browserify-shim": "^3.4.1",
    "mapify": "^0.1.0",
    "gulp": "^3.6.2",
    "vinyl-source-stream": "^0.1.1",
    "gulp-sass": "^0.7.1",
    "gulp-minify-css": "^0.3.1",
    "gulp-clean": "^0.2.4",
    "gulp-preprocess": "^1.0.1",
    "gulp-connect": "^2.0.5",
    "gulp-concat": "^2.2.0",
    "gulp-util": "^2.2.14",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^0.2.1",
    "envify": "^1.2.1"
  },
  "dependencies": {
    "react": "^0.10.0",
    "cortexjs": "^0.5.1",
    "underscore": "^1.6.0",
    "backbone": "^1.1.2",
    "jquery": "^2.1.0",
    "store": "^1.3.14",
    "backbone-validation": "^0.9.1",
    "express": "~3.5.1",
    "request": "^2.34.0",
    "node-jsx": "^0.10.0",
    "jswindow-shim": "0.0.0",
    "synapse-common": "git://github.com/synapsestudios/synapse-common",
    "mandrill-api": "^1.0.39",
    "redback": "^0.4.0",
    "cors": "^2.2.0",
    "validator": "^3.11.0",
    "loggly": "^1.0.4"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "underscore": "./node_modules/underscore/underscore.js",
    "backbone": "./node_modules/backbone/backbone.js",
    "react": "./node_modules/react/addons.js",
    "foundation": "./bower_components/foundation/js/foundation.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "underscore": "_",
    "backbone": {
      "exports": "Backbone",
      "depends": [
        "jquery:$",
        "underscore:_"
      ]
    },
    "foundation": {
      "exports": "",
      "depends": [
        "jquery"
      ]
    }
  }
}
